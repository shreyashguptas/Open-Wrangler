{
  "name": "open-wrangler",
  "displayName": "Open-Wrangler",
  "description": "Open-source data viewing and cleaning tool for VS Code",
  "version": "0.0.1",
  "publisher": "openwrangler",
  "license": "MIT",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Data Science",
    "Other"
  ],
  "activationEvents": [
    "onCommand:open-wrangler.open",
    "onCommand:open-wrangler.openFromCell",
    "onNotebook:*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "open-wrangler.open",
        "title": "Open-Wrangler: Open",
        "category": "Open-Wrangler"
      },
      {
        "command": "open-wrangler.openFromCell",
        "title": "Open-Wrangler: Open DataFrame from Cell",
        "category": "Open-Wrangler"
      }
    ],
    "notebookCellStatusBar": [
      {
        "viewType": "jupyter-notebook",
        "alignment": "left",
        "text": "Open in Open-Wrangler",
        "command": "open-wrangler.openFromCell",
        "when": "resourceExtname == .ipynb && notebookType == 'jupyter-notebook'"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.84.0",
    "typescript": "^5.4.5",
    "vsce": "^2.15.0"
  }
}
